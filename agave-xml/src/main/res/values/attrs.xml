<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ButtonMaterial99">
        <attr name="text" format="string"/>
        <attr name="enable" format="boolean"/>
        <attr name="type">
            <flag name="primary" value="0x00" />
            <flag name="secondary" value="0x01" />
            <flag name="tertiary" value="0x02" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ChipMaterial99">
        <attr name="textChip" format="string"/>
        <attr name="colorChip" format="color"/>
    </declare-styleable>
</resources>